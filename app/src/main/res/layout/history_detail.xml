<?xml version="1.0" encoding="utf-8"?>
<androidx.core.widget.NestedScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

    <View
        android:id="@+id/header_label"
        android:layout_width="match_parent"
        android:layout_height="56dp"
        android:background="@color/blue_primary"
        app:layout_constraintTop_toTopOf="parent"/>

    <ImageView
        android:id="@+id/imgBack"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="@id/header_label"
        app:layout_constraintBottom_toBottomOf="@id/header_label"
        app:layout_constraintLeft_toLeftOf="parent"
        android:src="@drawable/ic_back_white"
        android:layout_marginVertical="20dp"
        android:layout_marginLeft="20dp"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintLeft_toRightOf="@id/imgBack"
        android:text="History Detail"
        android:layout_marginLeft="37dp"
        android:fontFamily="@font/inter_bold"
        android:textSize="20sp"
        android:textColor="@color/white"
        app:layout_constraintTop_toTopOf="@id/header_label"
        app:layout_constraintBottom_toBottomOf="@id/header_label"/>

    <TextView
        android:id="@+id/nameProjectFunding"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@id/header_label"
        android:layout_marginTop="18dp"
        android:layout_marginLeft="20dp"
        android:text="Project funding"
        android:fontFamily="@font/inter_bold"
        android:textFontWeight="700"
        android:textSize="20sp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:textColor="@color/blue_primary"/>

    <TextView
        android:id="@+id/nominal"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@id/nameProjectFunding"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="6dp"
        android:text="Rp 5.000.000"
        app:layout_constraintLeft_toLeftOf="parent"
        android:textSize="20sp"
        android:fontFamily="@font/inter_bold"
        android:textColor="@color/hitam"/>

    <com.google.android.material.progressindicator.LinearProgressIndicator
        android:id="@+id/progressFunding"
        android:layout_marginHorizontal="25dp"
        android:layout_width="match_parent"
        android:layout_height="4dp"
        app:indicatorColor="@color/blue_primary"
        android:layout_marginTop="6dp"
        android:progress="75"
        app:layout_constraintTop_toBottomOf="@id/nominal"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"/>

    <TextView
        android:id="@+id/tvFa"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@id/progressFunding"
        android:layout_marginLeft="20dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:text="Funding amount"
        android:layout_marginTop="14dp"
        android:textSize="14sp"
        android:fontFamily="@font/inter_bold"
        android:textColor="@color/hitam"/>

    <TextView
        android:id="@+id/tvStatus"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Payment process"
        app:layout_constraintTop_toBottomOf="@id/progressFunding"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginRight="20dp"
        android:layout_marginTop="14dp"
        android:fontFamily="@font/inter_bold"
        android:textColor="@color/status"/>

    <TextView
        android:id="@+id/tvFundingAmountNominal"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Rp 500.000"
        android:fontFamily="@font/inter_bold"
        android:textSize="12sp"
        app:layout_constraintTop_toBottomOf="@id/tvFa"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="10dp"
        android:textColor="@color/hitam"/>

    <TextView
        android:id="@+id/tvDate"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="@color/status"
        app:layout_constraintTop_toBottomOf="@id/tvStatus"
        app:layout_constraintRight_toRightOf="parent"
        android:text="Sunday, 12 July 2022"
        android:layout_marginTop="10dp"
        android:layout_marginRight="20dp"
        android:fontFamily="@font/inter_bold"
        android:textSize="12sp"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/tvReturningMethod"
        android:text="Returning Method"
        app:layout_constraintTop_toBottomOf="@id/tvFundingAmountNominal"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="30dp"
        android:fontFamily="@font/inter_bold"
        app:layout_constraintLeft_toLeftOf="parent"
        android:textColor="@color/hitam"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="6 months installment"
        app:layout_constraintTop_toBottomOf="@id/tvDate"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginRight="20dp"
        android:layout_marginTop="30dp"
        android:fontFamily="@font/inter_regular"
        android:textColor="@color/hitam"
        android:textSize="12sp"/>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintpembayaran"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@id/tvReturningMethod">

        <TextView
            android:id="@+id/tvBca"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            android:text="BCA Virtual Account"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="20dp"
            android:fontFamily="@font/inter_bold"
            android:textColor="@color/blue_primary"
            android:textSize="18sp"/>

        <TextView
            android:id="@+id/tvVAN"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:layout_constraintTop_toBottomOf="@id/tvBca"
            app:layout_constraintLeft_toLeftOf="parent"
            android:layout_marginTop="10dp"
            android:text="Virtual Account Number"
            android:layout_marginLeft="20dp"
            android:fontFamily="@font/inter_bold"/>

        <TextView
            android:id="@+id/tvvirtualnumber"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintTop_toBottomOf="@id/tvVAN"
            app:layout_constraintLeft_toLeftOf="parent"
            android:layout_marginTop="10dp"
            android:layout_marginLeft="20dp"
            android:text="1394837285038294"
            android:fontFamily="@font/inter_bold"
            android:textColor="@color/hitam"
            android:textSize="18sp"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Salin"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintLeft_toRightOf="@id/tvvirtualnumber"
            app:layout_constraintTop_toBottomOf="@id/tvVAN"
            android:layout_marginTop="10dp"
            android:layout_marginEnd="20dp"
            android:fontFamily="@font/inter_regular"
            android:textSize="14sp"
            android:textColor="@color/blue_primary"/>

        <TextView
            android:id="@+id/tvTagihan"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:layout_constraintTop_toBottomOf="@id/tvvirtualnumber"
            app:layout_constraintLeft_toLeftOf="parent"
            android:layout_marginTop="10dp"
            android:text="Tagihan"
            android:layout_marginLeft="20dp"
            android:fontFamily="@font/inter_bold"/>

        <TextView
            android:id="@+id/tvjumlahtagihan"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintTop_toBottomOf="@id/tvTagihan"
            app:layout_constraintLeft_toLeftOf="parent"
            android:layout_marginTop="10dp"
            android:layout_marginLeft="20dp"
            android:text="50.000"
            android:fontFamily="@font/inter_bold"
            android:textColor="@color/hitam"
            android:textSize="18sp"/>

        <com.google.android.material.button.MaterialButton
            android:id="@+id/button_upload_receipt"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:backgroundTint="@color/blue_primary"
            android:text="Upload receipt of payment"
            android:textAllCaps="false"
            android:layout_marginHorizontal="20dp"
            android:layout_marginTop="20dp"
            app:layout_constraintTop_toBottomOf="@id/tvjumlahtagihan"
            android:fontFamily="@font/inter_bold"/>

    </androidx.constraintlayout.widget.ConstraintLayout>

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/constraintlate"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="28dp"
            app:layout_constraintTop_toBottomOf="@id/constraintpembayaran"
            tools:layout_editor_absoluteX="156dp">

            <TextView
                android:id="@+id/tvReturnInformation"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="20dp"
                android:layout_marginTop="20dp"
                android:fontFamily="@font/inter_bold"
                android:text="Return Information"
                android:textColor="@color/blue_primary"
                android:textSize="18sp"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <com.google.android.material.progressindicator.LinearProgressIndicator
                android:id="@+id/progressReturn"
                android:layout_width="match_parent"
                android:layout_height="4dp"
                android:layout_marginHorizontal="20dp"
                android:layout_marginTop="6dp"
                android:progress="60"
                app:indicatorColor="@color/blue_primary"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintTop_toBottomOf="@id/tvReturnInformation" />

            <TextView
                android:id="@+id/tvLoanRefund"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="20dp"
                android:layout_marginTop="10dp"
                android:fontFamily="@font/inter_bold"
                android:text="Loan Refund"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintTop_toBottomOf="@id/progressReturn" />

            <TextView
                android:id="@+id/tvRefundMethod"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="20dp"
                android:layout_marginTop="10dp"
                android:layout_marginRight="20dp"
                android:fontFamily="@font/inter_bold"
                android:text="Refund Method"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintLeft_toRightOf="@id/tvLoanRefund"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintTop_toBottomOf="@id/progressReturn" />

            <TextView
                android:id="@+id/tvAmountLoan"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="20dp"
                android:layout_marginTop="8dp"
                android:fontFamily="@font/inter_bold"
                android:text="50.000"
                android:textColor="@color/hitam"
                android:textSize="14sp"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintTop_toBottomOf="@id/tvLoanRefund" />

            <TextView
                android:id="@+id/tvMonthInstallment"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="20dp"
                android:layout_marginTop="8dp"
                android:layout_marginRight="20dp"
                android:fontFamily="@font/inter_bold"
                android:text="6 months installment"
                android:textColor="@color/hitam"
                android:textSize="14sp"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintTop_toBottomOf="@id/tvRefundMethod" />

            <View
                android:id="@+id/viewlate"
                android:layout_width="match_parent"
                android:layout_height="48dp"
                android:layout_marginTop="20dp"
                android:background="#EF5350"
                app:layout_constraintTop_toBottomOf="@id/tvAmountLoan" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="20dp"
                android:fontFamily="@font/inter_bold"
                android:text="Late Payment"
                android:textColor="@color/white"
                android:textSize="20sp"
                app:layout_constraintBottom_toBottomOf="@id/viewlate"
                app:layout_constraintTop_toTopOf="@id/viewlate" />
        </androidx.constraintlayout.widget.ConstraintLayout>

        <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@id/constraintlate">
        <TextView
            android:id="@+id/tvLoanInformation"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            android:text="Loan Information"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="20dp"
            android:fontFamily="@font/inter_bold"
            android:textColor="@color/blue_primary"
            android:textSize="18sp"/>

        <androidx.cardview.widget.CardView
            android:id="@+id/cardviewloanapp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginHorizontal="20dp"
            android:layout_marginTop="10dp"
            app:cardElevation="5dp"
            app:cardCornerRadius="10dp"
            app:layout_constraintTop_toBottomOf="@id/tvLoanInformation">
            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <ImageView
                    android:id="@+id/ic_logo_loaninformation"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="22dp"
                    android:layout_marginTop="22dp"
                    android:layout_marginBottom="22dp"
                    android:src="@drawable/ic_google"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <TextView
                    android:id="@+id/loanapp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Loan Application"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintLeft_toRightOf="@id/ic_logo_loaninformation"
                    android:layout_marginLeft="20dp"
                    android:layout_marginTop="21dp"
                    android:fontFamily="@font/inter_regular"
                    android:textSize="14sp"
                    android:textColor="#3e3e3e"
                    />
                <TextView
                    android:id="@+id/text_history_start_up_name"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Star Track.inc"
                    app:layout_constraintLeft_toRightOf="@id/ic_logo_loaninformation"
                    android:layout_marginTop="5dp"
                    app:layout_constraintTop_toBottomOf="@id/loanapp"
                    android:layout_marginLeft="20dp"
                    android:fontFamily="@font/inter_bold"
                    android:textSize="20sp"
                    android:textColor="@color/blue_primary"/>

            </androidx.constraintlayout.widget.ConstraintLayout>

        </androidx.cardview.widget.CardView>

        <androidx.cardview.widget.CardView
            android:id="@+id/cardview_fundingneed"
            app:layout_constraintTop_toBottomOf="@id/cardviewloanapp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginHorizontal="20dp"
            android:layout_marginTop="10dp"
            app:cardCornerRadius="10dp"
            app:cardElevation="5dp">

            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginLeft="20dp">

                <TextView
                    android:id="@+id/tvfundingneed"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Funding Needs"
                    app:layout_constraintTop_toTopOf="parent"
                    android:layout_marginTop="20dp"
                    app:layout_constraintLeft_toLeftOf="parent"
                    android:fontFamily="@font/inter_bold"
                    android:textSize="20sp"
                    android:textColor="@color/blue_primary"/>

                <TextView
                    android:id="@+id/tvcontentaboutstartup"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    app:layout_constraintTop_toBottomOf="@id/tvfundingneed"
                    android:layout_marginTop="8dp"
                    android:text="@string/content_funding_needs"
                    android:layout_marginHorizontal="20dp"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintRight_toRightOf="parent"
                    android:layout_marginBottom="20dp"
                    app:layout_constraintBottom_toBottomOf="parent"
                    android:fontFamily="@font/inter_regular"/>

            </androidx.constraintlayout.widget.ConstraintLayout>

        </androidx.cardview.widget.CardView>

        <androidx.cardview.widget.CardView
            android:id="@+id/cardview_aboutstartup"
            app:layout_constraintTop_toBottomOf="@id/cardview_fundingneed"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginHorizontal="20dp"
            android:layout_marginTop="10dp"
            app:cardCornerRadius="10dp"
            app:cardElevation="5dp">

            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginLeft="20dp">

                <TextView
                    android:id="@+id/tvAboutStartup"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="About Startup"
                    app:layout_constraintTop_toTopOf="parent"
                    android:layout_marginTop="20dp"
                    app:layout_constraintLeft_toLeftOf="parent"
                    android:fontFamily="@font/inter_bold"
                    android:textSize="20sp"
                    android:textColor="@color/blue_primary"/>

                <TextView
                    android:id="@+id/tvDetailAboutStartup"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    app:layout_constraintTop_toBottomOf="@id/tvAboutStartup"
                    android:layout_marginTop="8dp"
                    android:text="@string/content_aboutstartup"
                    android:layout_marginHorizontal="20dp"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintRight_toRightOf="parent"
                    android:fontFamily="@font/inter_regular"/>

                <androidx.cardview.widget.CardView
                    android:id="@+id/cardview_addressstartup"
                    app:layout_constraintTop_toBottomOf="@id/tvDetailAboutStartup"
                    android:layout_width="match_parent"
                    android:layout_height="65dp"
                    android:layout_marginHorizontal="20dp"
                    android:layout_marginVertical="10dp"
                    app:cardCornerRadius="5dp"
                    app:cardElevation="5dp">

                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent">

                        <TextView
                            android:id="@+id/tvaddress"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            app:layout_constraintTop_toTopOf="parent"
                            android:layout_marginTop="8dp"
                            android:text="Address"
                            android:layout_marginLeft="14dp"
                            app:layout_constraintLeft_toLeftOf="parent"
                            android:fontFamily="@font/inter_regular"/>

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            app:layout_constraintTop_toTopOf="parent"
                            android:layout_marginTop="8dp"
                            android:text="4rd Sudirman St., Jakarta"
                            android:layout_marginRight="16dp"
                            app:layout_constraintRight_toRightOf="parent"
                            android:fontFamily="@font/inter_regular"/>

                        <TextView
                            android:id="@+id/tvsince"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="8dp"
                            android:text="Since"
                            android:layout_marginLeft="14dp"
                            app:layout_constraintTop_toBottomOf="@id/tvaddress"
                            app:layout_constraintLeft_toLeftOf="parent"
                            android:fontFamily="@font/inter_regular"/>

                        <TextView
                            android:id="@+id/addressstartup"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            app:layout_constraintTop_toTopOf="parent"
                            android:layout_marginTop="8dp"
                            android:text="4rd Sudirman St., Jakarta"
                            android:layout_marginRight="16dp"
                            app:layout_constraintRight_toRightOf="parent"
                            android:fontFamily="@font/inter_regular"/>

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            app:layout_constraintTop_toBottomOf="@id/addressstartup"
                            android:layout_marginTop="8dp"
                            android:layout_marginBottom="10dp"
                            android:text="2019"
                            android:layout_marginRight="16dp"
                            app:layout_constraintRight_toRightOf="parent"
                            android:fontFamily="@font/inter_regular"/>

                    </androidx.constraintlayout.widget.ConstraintLayout>
                </androidx.cardview.widget.CardView>
                <androidx.recyclerview.widget.RecyclerView
                    app:layout_constraintTop_toBottomOf="@id/cardview_addressstartup"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
                    android:layout_marginTop="20dp"
                    android:layout_marginBottom="20dp"
                    tools:listitem="@layout/adapter_history_detail_image"/>

            </androidx.constraintlayout.widget.ConstraintLayout>

        </androidx.cardview.widget.CardView>

            <androidx.cardview.widget.CardView
                android:id="@+id/cardview_contact"
                app:layout_constraintTop_toBottomOf="@id/cardview_aboutstartup"
                android:layout_width="match_parent"
                android:layout_height="204dp"
                android:layout_marginHorizontal="20dp"
                android:layout_marginTop="10dp"
                app:cardElevation="5dp"
                app:cardCornerRadius="10dp">

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="20dp">

                    <TextView
                        android:id="@+id/tvContactLabel"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="Contact"
                        app:layout_constraintTop_toTopOf="parent"
                        android:layout_marginTop="20dp"
                        app:layout_constraintLeft_toLeftOf="parent"
                        android:fontFamily="@font/inter_bold"
                        android:textSize="20sp"
                        android:textColor="@color/blue_primary"/>

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        app:layout_constraintTop_toBottomOf="@id/tvContactLabel"
                        android:id="@+id/tvCweb"
                        android:text="Website"
                        android:layout_marginTop="8dp"
                        app:layout_constraintLeft_toLeftOf="parent"/>

                     <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="8dp"
                        android:layout_marginEnd="20dp"
                        app:layout_constraintTop_toBottomOf="@id/tvContactLabel"
                        android:id="@+id/tvContentWeb"
                        android:text="www.starttrack.com"
                        app:layout_constraintRight_toRightOf="parent"/>

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        app:layout_constraintTop_toBottomOf="@id/tvCweb"
                        android:id="@+id/tvCPhone"
                        android:text="Phone"
                        android:layout_marginTop="8dp"
                        app:layout_constraintLeft_toLeftOf="parent"/>

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="8dp"
                        app:layout_constraintTop_toBottomOf="@id/tvContentWeb"
                        android:id="@+id/tvContentPhone"
                        android:text="081234567890"
                        android:layout_marginEnd="20dp"
                        app:layout_constraintRight_toRightOf="parent"/>

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        app:layout_constraintTop_toBottomOf="@id/tvCPhone"
                        android:id="@+id/tvclinkedin"
                        android:text="LinkedIn"
